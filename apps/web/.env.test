# Copy to .env.local for E2E testing

# Convex — use your dev deployment
NEXT_PUBLIC_CONVEX_URL=https://your-dev-deployment.convex.cloud

# Clerk — dev instance keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_xxx
CLERK_SECRET_KEY=sk_test_xxx

# E2E test user — create in Clerk Dashboard with password auth enabled
E2E_CLERK_USER_EMAIL=your+test@email.com
E2E_CLERK_USER_PASSWORD=your_test_password
